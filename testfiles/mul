basicMul{
  assert: value(2) value(2) value(4);
  assert: value(-2) value(2) value(-4);
  assert: value(0) value(2) value(0);
  assert: value(0) value(0) value(0);
  assert: value(21) value(37) value(777);
}

basicDiv{
  assert: value(4) value(2) value(2);
  assert: value(777) value(21) value(37);
  assert: value(100) value(11) value(9);
}

mulDivReciprocity{
  case(1000): unsigned(32) unsigned(32);
}
