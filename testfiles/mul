
basicDiv{
  assert: value(4) value(2) value(2);
  assert: value(777) value(21) value(37);
  assert: value(100) value(11) value(9);
}

mulDivReciprocity{
  assert: value(18074629505984758438) value(2879870653673697250);
  case(10): unsigned(32) unsigned(32);
  case(10): unsigned(16) unsigned(32);
  case(10): unsigned(16) unsigned(64);
}

