equality{
  assert: value(10) value(10);
  !assert: value(10) value(-10);
  assert: value(37) value(37);
  assert: value(-34) value(-34);
  !assert: value(-35) value(-34);
  assert: value(0) value(0);
  assert: value(-0) value(0);
}

addition{
  case(100): unsigned(8) range(0,10);
  case(100): unsigned(10) signed(8);
  case0(10): signed(4) unsigned(4);
}
subtraction{
  assert: value(850395763712691800) value(850395763712691800) value(0);
}
